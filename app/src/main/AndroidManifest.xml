<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.figurehowto.gadsleaderboard">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LeaderboardActivity" />
        <activity
            android:name=".AreYouSure"
            android:theme="@style/AppTheme.TransLucentTheme" />
        <activity
            android:name=".SubmitFailed"
            android:theme="@style/AppTheme.TransLucentTheme" />
        <activity
            android:name=".SubmitSucessful"
            android:theme="@style/AppTheme.TransLucentTheme" />
        <activity
            android:name=".ProjectSubmissionActivity"
            android:theme="@style/SubmitActivityTheme" />
    </application>



</manifest>